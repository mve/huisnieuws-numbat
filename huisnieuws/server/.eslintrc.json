{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "project": [
      "**/tsconfig.json"
    ]
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-console": [
      "error",
      {
        "allow": [
          "info",
          "time",
          "timeEnd"
        ]
      }
    ],
    "no-underscore-dangle": "off",
    "linebreak-style": 0,
    "import/prefer-default-export": "off",
    "@typescript-eslint/no-unused-expressions": "off"
  }
}
